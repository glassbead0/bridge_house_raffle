<div class="page-header">
  <%= link_to tickets_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_ticket_path(@ticket), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show ticket</h1>
</div>

<dl class="dl-horizontal">
  <dt>First name:</dt>
  <dd><%= @ticket.first_name %></dd>

  <dt>Last name:</dt>
  <dd><%= @ticket.last_name %></dd>

  <dt>Email:</dt>
  <dd><%= @ticket.email %></dd>

  <dt>Subscribe:</dt>
  <dd><%= @ticket.subscribe %></dd>

  <dt>Address line one:</dt>
  <dd><%= @ticket.address_line_one %></dd>

  <dt>Address line two:</dt>
  <dd><%= @ticket.address_line_two %></dd>

  <dt>City:</dt>
  <dd><%= @ticket.city %></dd>

  <dt>State:</dt>
  <dd><%= @ticket.state %></dd>

  <dt>Zip code:</dt>
  <dd><%= @ticket.zip_code %></dd>

</dl>
